{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/zenigo-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "js-widget/**/*"
    ],
    "webpack": true,
    "tsConfigPath": "apps/zenigo-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/zenigo-api",
  "projects": {
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "constants": {
      "type": "library",
      "root": "libs/constants",
      "entryFile": "index",
      "sourceRoot": "libs/constants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constants/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "dto": {
      "type": "library",
      "root": "libs/dto",
      "entryFile": "index",
      "sourceRoot": "libs/dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dto/tsconfig.lib.json"
      }
    },
    "echonexus-api": {
      "type": "application",
      "root": "apps/echonexus-api",
      "entryFile": "main",
      "sourceRoot": "apps/echonexus-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/echonexus-api/tsconfig.app.json"
      }
    },
    "init": {
      "type": "library",
      "root": "libs/init",
      "entryFile": "index",
      "sourceRoot": "libs/init/src",
      "compilerOptions": {
        "tsConfigPath": "libs/init/tsconfig.lib.json"
      }
    },
    "payments": {
      "type": "library",
