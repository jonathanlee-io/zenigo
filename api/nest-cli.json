      }
    },
    "init": {
      "type": "library",
      "root": "libs/init",
      "entryFile": "index",
      "sourceRoot": "libs/init/src",
      "compilerOptions": {
        "tsConfigPath": "libs/init/tsconfig.lib.json"
      }
    },
    "payments-service": {
      "type": "application",
      "root": "apps/payments-service",
      "entryFile": "main",
      "sourceRoot": "apps/payments-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payments-service/tsconfig.app.json"
      }
    },
    "storage": {
      "type": "library",
      "root": "libs/storage",
      "entryFile": "index",
