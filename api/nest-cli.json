{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/feature-flags-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "js-widget/**/*"
    ],
    "webpack": true,
    "tsConfigPath": "apps/feature-flags-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/feature-flags-service",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "constants": {
      "type": "library",
      "root": "libs/constants",
      "entryFile": "index",
      "sourceRoot": "libs/constants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constants/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "dto": {
      "type": "library",
      "root": "libs/dto",
      "entryFile": "index",
      "sourceRoot": "libs/dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dto/tsconfig.lib.json"
      }
    },
    "feature-flags-service": {
      "type": "application",
      "root": "apps/feature-flags-service",
      "entryFile": "main",
      "sourceRoot": "apps/feature-flags-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/feature-flags-service/tsconfig.app.json"
      }
    },
    "feedback-service": {
      "type": "application",
      "root": "apps/feedback-service",
      "entryFile": "main",
      "sourceRoot": "apps/feedback-service/src",
