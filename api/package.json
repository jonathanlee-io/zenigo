  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "widget:build": "npx webpack --config ./widget/webpack.config.js --env=development",
    "widget:build:prod": "npx webpack --config ./widget/webpack.config.js --env=production",
    "widget:start": "npx webpack-dev-server --open",
    "build": "yarn run orchestrate:build",
    "orchestrate:build": "concurrently -c \"bgGreen.bold,blue.bold,cyan.bold,green.bold,magenta.bold\" yarn:orchestrate:build:*",
    "orchestrate:build:api-gateway": "nest build api-gateway",
    "orchestrate:build:feature-flags-service": "cd apps/feature-flags-service && npx prisma generate && cd ../.. && nest build feature-flags-service",
    "orchestrate:build:feedback-service": "cd apps/feedback-service && npx prisma generate && cd ../.. && nest build feedback-service",
    "orchestrate:build:identity-service": "cd apps/identity-service && npx prisma generate && cd ../.. && nest build identity-service",
    "orchestrate:build:payments-service": "cd apps/payments-service && npx prisma generate && cd ../.. && nest build payments-service ",
    "orchestrate:start": "concurrently -c \"bgGreen.bold,blue.bold,cyan.bold,green.bold,magenta.bold\" yarn:orchestrate:start:*",
    "orchestrate:start:api-gateway": "nest start api-gateway --watch",
    "orchestrate:start:feature-flags-service": "nest start feature-flags-service --watch",
    "orchestrate:start:feedback-service": "nest start feedback-service --watch",
    "orchestrate:start:identity-service": "nest start identity-service --watch",
    "orchestrate:start:payments-service": "nest start payments-service --watch",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "yarn run orchestrate:build && jest --config ./jest.config.ts --coverage",
    "test:watch": "yarn run orchestrate:build && jest --watch",
    "test:cov": "yarn run orchestrate:build && jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky"
  },
